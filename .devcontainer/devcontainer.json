{
  "name": "harser",
  "image": "j3m3/haskell-devcontainer:base",
  "runArgs": [
    "--name",
    "${localEnv:USER}-harser"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "haskell.haskell",
        "phoityne.phoityne-vscode",
        "justusadam.language-haskell",
        "hoovercj.haskell-linter"
      ]
    }
  }
}
